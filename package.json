    "wd:platforms:groupings": "tsx scripts/wikidata/importPlatformGroupings.ts",
    "wd:games:reset": "tsx scripts/wikidata/fetchGamesByPlatform.ts --reset-cursor",
    "wd:games": "tsx scripts/wikidata/fetchGamesByPlatform.ts",
    "wd:enrich": "tsx scripts/wikidata/enrichGames.ts",
    "wd:enrich:all": "tsx scripts/wikidata/enrichGames.ts --all",
    "wd:analyze-props": "tsx scripts/wikidata/analyzePropertyUsage.ts",
    "wd:hydrate-prop-meta": "tsx scripts/wikidata/hydratePropertyMetadata.ts",
